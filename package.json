{
  "name": "dapao",
  "version": "0.1.0",
  "description": "A lightweight 3D physics engine written in Typescript (Port of Cannon.js written in Javascript)",
  "homepage": "https://github.com/TheRohans/DaPao",
  "author": "Stefan Hedman <schteppe@gmail.com> (http://steffe.se)",
  "contributors": [
    "Rob Rohan <rob@therohans.com> (https://robrohan.com)"
  ],
  "license": "MIT",
  "keywords": [
    "dapao",
    "cannon.js",
    "cannon",
    "physics",
    "engine",
    "3d"
  ],
  "scripts": {
    "install:deps": "npm install --no-optional",
    "test": "NODE_ENV=test karma start --log-level debug --browsers ChromeHeadless --single-run --reporters=spec",
    "test:full": "NODE_ENV=test karma start --log-level debug --browsers ChromeHeadless --single-run --code-coverage --reporters=spec,kjhtml,karma-remap-istanbul",
    "build": "NODE_ENV=production node node_modules/.bin/webpack --display-error-details --progress -p",
    "lint": "tslint --config tslint.json --project ."
  },
  "main": "./lib/dapao.js",
  "types": "./lib/types/main.d.ts",
  "engines": {
    "node": "*"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:TheRohans/dapao.git"
  },
  "bugs": {
    "url": "https://github.com/TheRohans/dapao/issues"
  },
  "devDependencies": {
    "ts-loader": "^2.3.4",
    "tslint": "^5.0.0",
    "eslint": "^4.16.0",
    "typescript": "^2.5.3",
    "webpack": "^3.5.5",
    "nodeunit": "^0.9.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "strip-loader": "^0.1.2",
    "@types/jasmine": "^2.6.0",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^3.2.0",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-remap-istanbul": "^0.6.0",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.4"
  },
  "dependencies": {}
}
